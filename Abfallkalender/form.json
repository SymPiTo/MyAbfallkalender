{
  "elements": [
    {
      "type": "ValidationTextBox",
      "name": "RemoteFile",
      "caption": "Pfad zur ICS-Datei auf dem Windows-Rechner",
      "width": "600px"
    },
    {
      "type": "Label",
      "caption": "Beispiel: \\\\WINPC\\Abfall\\heidelberg.ics"
    },
    {
      "type": "ValidationTextBox",
      "name": "LocalFile",
      "caption": "Lokaler Speicherpfad in Symcon",
      "width": "600px"
    },
    {
      "type": "Label",
      "caption": "Beispiel: /var/lib/symcon/modules/Torsten.HeidelbergAbfall/abfall.ics"
    }
  ],
  "actions": [
    {
      "type": "Button",
      "caption": "ICS jetzt holen und verarbeiten",
      "onClick": "HDABF_Update($id);"
    }
  ]
}
